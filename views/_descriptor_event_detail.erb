<% if @result['descriptor_validation']["descriptor_valid"] == "failure" %>
  <% if not @result["descriptor_validation"]["validator_output"].empty? %>
    <eventOutcomeDetail>
      <eventOutcomeDetailNote>
      <% @result["descriptor_validation"]["validator_output"].each do |line| %>
        <%= line %>
      <% end  %>
      </eventOutcomeDetailNote>
    </eventOutcomeDetail>
  <% else %>
    <eventOutcomeDetail>
      <eventOutcomeDetailNote>
      Unable to validate package descriptor. This error typically results from attempting to validate non-well formed XML.
      </eventOutcomeDetailNote>
    </eventOutcomeDetail>
  <% end %>
<% end %>
