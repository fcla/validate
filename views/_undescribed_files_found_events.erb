<% @result['undescribed_files'].each do |value| %>
  <event>
    <eventIdentifier>
      <eventIdentifierType>Temporary Local</eventIdentifierType>
      <eventIdentifierValue><%= @auto_incrementer.get_next %></eventIdentifierValue>
    </eventIdentifier>
    <eventType>Undescribed file found in SIP</eventType>
    <eventDateTime><%= Time.now %></eventDateTime>
    <linkingAgentIdentifier>
      <linkingAgentIdentifierType>URI</linkingAgentIdentifierType>
      <linkingAgentIdentifierValue>http://daitss/validation</linkingAgentIdentifierValue>
    </linkingAgentIdentifier>
    <linkingObjectIdentifier>
      <linkingObjectIdentifierType>URI</linkingObjectIdentifierType>
      <linkingObjectIdentifierValue>
        file://<%= File.join(@result["path_to_aip"], value) %>
      </linkingObjectIdentifierValue>
    </linkingObjectIdentifier>
  </event>
<% end %>
